{
  "$schema": "http://json.schemastore.org/template",
  "author": "lxy",
  "classifications": [
    "Duckov",
    "Mod",
    "Library"
  ],
  "identity": "Duckov.Mod.CSharp",
  "name": "Duckov Mod",
  "shortName": "Duckovmod",
  "tags": {
    "language": "C#",
    "type": "project"
  },
  "sourceName": "DuckovModTemplate",
  "preferNameDirectory": true,
  "symbols": {
    "UsePolyfill": {
      "type": "parameter",
      "datatype": "bool",
      "defaultValue": "true",
      "description": "Whether to use Polyfill package or not"
    }
  },
  "sources": [
    {
      "exclude": [
        ".idea/**/*",
        ".template.config/**/*",
        "bin/**/*",
        "obj/**/*"
      ]
    }
  ]
}